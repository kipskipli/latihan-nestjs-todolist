{
  "name": "inspigo-nestjs-boilerplate",
  "version": "0.0.1",
  "description": "Inspigo NestJs Boilerplate",
  "author": "Inspigo",
  "private": true,
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run env:dev && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run env:test && jest",
    "test:watch": "npm run env:test && jest --watch",
    "test:cov": "npm run env:test && jest --coverage",
    "test:debug": "npm run env:test && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run env:test && jest --config ./test/jest-e2e.json",
    "env:test": "cp .env.test .env",
    "env:dev": "cp .env.dev .env"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^1.17.1",
    "@inspigoid/inspigo-utils-ts": "^1.0.80",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "amqplib": "^0.8.0",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "nestjs-redis": "^1.3.3",
    "helmet": "^4.6.0",
    "joi": "^17.4.0",
    "mongoose": "^5.12.14",
    "passport": "^0.4.1",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/amqplib": "^0.8.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/joi": "^17.2.3",
    "@types/node": "^14.14.36",
    "@types/passport-jwt": "^3.0.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "run-script-webpack-plugin": "0.0.11",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "webpack": "^5.39.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inspigo/inspigo-api-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/inspigo/inspigo-api-boilerplate/issues"
  },
  "homepage": "https://github.com/inspigo/inspigo-api-boilerplate#readme"
}
